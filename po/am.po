#: ../pay-ui/app/ui/CheckoutPage.qml:27
msgid "Payment"
msgstr "ክፍያ"

#: ../pay-ui/app/ui/CheckoutPage.qml:228
msgid "Enter your Ubuntu One password"
msgstr "የ እርስዎን የ ኡቡንቱ ዋን የ መግቢያ ቃል ያስገቡ"

#: ../pay-ui/app/ui/CheckoutPage.qml:271
msgid "Type your verification code:"
msgstr "የ እርስዎን ማረጋገጫ ኮድ ይጻፉ:"

#: ../pay-ui/app/ui/CheckoutPage.qml:282
msgid "2-factor device code"
msgstr "2-ፋክተር የ አካል ኮድ"

#: ../pay-ui/app/ui/CheckoutPage.qml:370 ../pay-ui/app/payui.qml:342
msgid "Cancel"
msgstr "መሰረዣ"

#: ../pay-ui/app/ui/CheckoutPage.qml:379
msgid "Buy Now"
msgstr "አሁን መግዣ"

#: ../pay-ui/app/ui/CheckoutPage.qml:393
msgid "Add credit/debit card"
msgstr "ክሬዲት/ዴቢት ካርድ መጨመሪያ"

#: ../pay-ui/app/ui/ErrorDialog.qml:45
msgid "Retry"
msgstr "እንደገና ይሞክሩ"

#: ../pay-ui/app/ui/ErrorDialog.qml:55
msgid "Close"
msgstr "መዝጊያ"

#: ../pay-ui/app/payui.qml:147
msgid "Finish Purchase"
msgstr "ግዢውን መጨረሻ"

#: ../pay-ui/app/payui.qml:162
msgid "Incorrect Password, please try again."
msgstr "የተሳሳተ የመግቢያ ቃል እባክዎን እንደገና ይሞክሩ"

#: ../pay-ui/app/payui.qml:276
msgid "Purchase failed"
msgstr "ግዢው አልተሳካም"

#: ../pay-ui/app/payui.qml:277
msgid "The purchase couldn't be completed."
msgstr "ግዢውን መፈጸም አልተቻለም"

#: ../pay-ui/app/payui.qml:292
msgid "Error contacting the server"
msgstr "ስህተት ተፈጥሯል ሰርቨሩን በመገናኘት ላይ እንዳለ"

#: ../pay-ui/app/payui.qml:293 ../pay-ui/app/payui.qml:309
msgid "Do you want to try again?"
msgstr "እንደገና መሞከር ይፈልጋሉ?"

#: ../pay-ui/app/payui.qml:308
msgid "Adding Credit Card failed"
msgstr "የ ክሬዲር ካርድ መጨመር አልተቻለም"

#: ../pay-ui/app/payui.qml:327
msgid "Processing Purchase"
msgstr "ግዢውን በማካሄድ ላይ"

#: ../pay-ui/app/payui.qml:327
msgid "Loading"
msgstr "በመጫን ላይ"

#: ../pay-ui/app/payui.qml:328
msgid "Please wait…"
msgstr "እባክዎን ይጠብቁ…"

#: ../pay-ui/app/payui.qml:403
msgid "Add Payment"
msgstr "ክፍያውን መጨመሪያ"

